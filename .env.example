# OSCAR Environment Configuration
# Copy this file to .env and add your API keys

# Required: LLM API Keys (at least one required)
GROQ_API_KEY=your_groq_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Tavily API Keys for Web Search (dual keys for rate limit fallback)
TAVILY_API_KEY1=your_primary_tavily_key
TAVILY_API_KEY2=your_backup_tavily_key

# Optional: Application Settings
OSCAR_LOG_LEVEL=INFO
OSCAR_DATA_DIR=./data
OSCAR_SAFE_MODE=true

# Optional: Development Settings  
OSCAR_DEBUG=false
OSCAR_DRY_RUN=false